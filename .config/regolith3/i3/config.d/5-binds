bindsym $mod+Shift+å exec "ssh pi@192.168.100.35 tv_off"

bindsym $alt+v exec "~/Scripts/greenclip"
bindsym $mod+Esc exec "~/Scripts/chrome-reload"
bindsym $mod+F11 exec "python ~/Scripts/plug.py"
bindsym $mod+F12 exec "~/Scripts/pulse-toggle"

bindsym $alt+c exec gnome-calendar
# bindsym $alt+p exec "kitty --title float -e python"
bindsym $alt+p exec "kitty  -e python"
bindsym $alt+w exec "kitty -e 'source /home/smaipe/.bash_aliases && work'"
bindsym $alt+s exec spotify
